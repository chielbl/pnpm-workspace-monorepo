{
  "name": "fastify-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/server.ts",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "db:introspect": "drizzle-kit introspect:pg --config=./src/db/drizzle.config.ts",
    "db:generate": "drizzle-kit generate:pg --config=./src/db/drizzle.config.ts",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:seed": "tsx ./src/db/seed.ts",
    "db:drop": "drizzle-kit drop --config=./src/db/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --verbose --config=./src/db/drizzle.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@repo/config": "workspace:^",
    "@sinclair/typebox": "^0.32.20",
    "@t3-oss/env-core": "^0.9.2",
    "@vercel/postgres": "^0.8.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.8",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "pg": "^8.11.5",
    "pino": "^8.20.0",
    "postgres": "^3.4.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fastify-cors": "^2.1.0",
    "@types/node": "^20.12.2",
    "drizzle-kit": "^0.20.14",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
